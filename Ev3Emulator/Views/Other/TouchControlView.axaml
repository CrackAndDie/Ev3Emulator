<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:prism="http://prismlibrary.com/"
		     xmlns:prismRegions="clr-namespace:Prism.Regions;assembly=Prism.Avalonia"
             prism:ViewModelLocator.AutoWireViewModel="True"
			 mc:Ignorable="d" 
			 d:DesignWidth="800" 
			 d:DesignHeight="450"
             x:Class="Ev3Emulator.Views.Other.TouchControlView">
	<Grid ColumnDefinitions="*">
		<Button Content="Touch"
				Theme="{StaticResource MainButton}"
				Name="touchButton"
				FontWeight="Medium"
				VerticalAlignment="Center"
				HorizontalAlignment="Left"
                Foreground="Black"
				FontFamily="{StaticResource MontsFont}"
				Padding="5"/>
	</Grid>
</UserControl>
